{% extends 'base_back.html.twig' %}

{% block title %}Oeuvre{% endblock %}

{% block body %}
    <h1>Oeuvre</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ oeuvre.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ oeuvre.titre }}</td>
            </tr>
            <tr>
                <th>Anne_creation</th>
                <td>{{ oeuvre.anneCreation }}</td>
            </tr>
            <tr>
                <th>Dimention</th>
                <td>{{ oeuvre.dimention }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ oeuvre.prix }}</td>
            </tr>
            <tr>
                <th>Disponibilite</th>
                <td>{{ oeuvre.disponibilite ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_oeuvre_index') }}">back to list</a>

    <a href="{{ path('app_oeuvre_edit', {'id': oeuvre.id}) }}">edit</a>

    {{ include('oeuvre/_delete_form.html.twig') }}
{% endblock %}
